{
  "name": "jsok-vault",
  "version": "1.1.8",
  "author": "jsok",
  "summary": "Puppet module to manage Vault (https://vaultproject.io)",
  "license": "Apache-2.0",
  "source": "https://github.com/jsok/puppet-vault.git",
  "project_page": "https://github.com/jsok/puppet-vault",
  "issues_url": "https://github.com/jsok/puppet-vault/issues",
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.2.0 < 3.4.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 3.0.0 < 5.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.2.0 < 5.0.0"
    },
    {
      "name": "puppet-archive",
      "version_requirement": ">= 1.1.2 < 2.0.0"
    },
    {
      "name": "hfm-initsystem",
      "version_requirement": ">= 0.2.0 < 1.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
          "6.0",
          "7.0"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
          "6.0",
          "7.0"
      ]
    },
    {
      "operatingsystem": "Amazon",
      "operatingsystemrelease": [
        "2016.03"
      ]
    }
  ]
}
